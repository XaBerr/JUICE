<div class="col projectDetail">
  <div class="row">
    <div class="col" id="back">
      <a href="">
        <i class="fas fa-caret-square-left"></i>
      </a>
      <h1 class="text-center">{{projectName}}</h1>
    </div>
  </div>
  <hr>
  <div class="row">
    {% for project in projects %}
      <div class="col-lg-4 col-md-6 col-xs-12 project">
        <div class="card">
          <div class="card-body">
            <div class="row">
              <div class="col-lg text-message">
                {{project.message}}
              </div>
            </div>
          </div>
          <div class="card-footer">
            <small>Updated on
              {{ project.date }}
              by
              {{ project.author }}</small>
          </div>
        </div>
      </div>
    {% endfor %}
  </div>
</div>
