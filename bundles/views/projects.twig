<div class="col projects">
  {# Search bar #}
  <div class="row">
    <div class="col" id="search-bar">
      <div class="progress">
        <div class="progress-bar" role="progressbar" aria-valuemin="0" aria-valuemax="100"></div>
      </div>
      <a href="themes">
        <img src="img/brand/juice-logo.svg" alt="JUICE" width="40px">
      </a>
      <input name="search" type="text" placeholder="Search" id="search">
    </div>
  </div>
  {# Projects #}
  <div class="row" id="projects-list">
    {% for project in projects %}
      {{project|raw}}
    {% endfor %}
  </div>
</div>
